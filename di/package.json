{
  "name": "@caffeinejs/di",
  "version": "0.0.0",
  "description": "CaffeineJs DI Framework",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json",
    "./decorators": {
      "node": "./dist/decorators/index.js",
      "default": "./dist/decorators/index.js"
    },
    "./internal": {
      "node": "./dist/internal/index.js",
      "default": "./dist/internal/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc --build --force tsconfig.build.json",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --detectOpenHandles --coverage",
    "bench": "node --loader ts-node/esm _bench/bench.ts"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "devDependencies": {
    "@caffeinejs/internal-perf": "^0.0.0"
  }
}
